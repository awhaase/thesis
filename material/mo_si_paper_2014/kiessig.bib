	Publisher = {OSA},
	Title = {Molybdenum-silicon multilayer mirrors for the extreme ultraviolet},
	Url = {http://ao.osa.org/abstract.cfm?URI=ao-24-6-883},
	Volume = {24},
	Year = {1985},
	Bdsk-Url-1 = {http://ao.osa.org/abstract.cfm?URI=ao-24-6-883},
